# Supabase Configuration  
SUPABASE_URL= # Your Supabase project URL
SUPABASE_KEY= # Supabase service role key
SUPABASE_ANON_KEY= # Supabase anonymous public key

# Stripe Configuration
STRIPE_SECRET_KEY= # Stripe secret key (starts with sk_test or sk_live)
STRIPE_PUBLIC_KEY= # Stripe public key (starts with pk_test or pk_live)
STRIPE_WEBHOOK_SECRET= # Stripe webhook signing secret
MERCHANT_ID= # Stripe merchant identifier
MERCHANT_DISPLAY_NAME= # Merchant display name for UI

# eSIM Hub Configuration
ESIM_HUB_BASE_URL= # Base URL for primary eSIM hub API
ESIM_HUB_BASE_URL2= # Base URL for secondary eSIM hub API
ESIM_HUB_API_KEY= # API key for accessing the eSIM hub
ESIM_HUB_TENANT_KEY= # Tenant key for the eSIM hub integration

# Firebase Cloud Messaging
FCM_CONFIG_FILE= # Path to your Firebase FCM service account JSON file

# Global Configurations
DEFAULT_CURRENCY= # Default currency code (e.g. USD, EUR)
WHATSAPP_NUMBER= # WhatsApp support number in international format (e.g. 961XXXXXXXX)

# SMTP (Email) Configuration
SMTP_SERVER= # SMTP server address
SMTP_PORT= # SMTP server port (e.g. 587)
SMTP_USERNAME= # SMTP authentication username
SMTP_PASSWORD= # SMTP authentication password
SMTP_SENDER= # Default sender email address

# Referral System
DEFAULT_REFERRAL_RULE_ID= # Default referral rule UUID
REFERRAL_CODE_AMOUNT= # Amount rewarded per referral

# Currency Exchange API
CURRENCY_URL= # URL for currency conversion API with placeholders for values
